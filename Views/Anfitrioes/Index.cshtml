@model IEnumerable<PortariaInteligente.Models.Anfitriao>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.nomeAnfitriao)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.emailAnfitriao)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.celAnfitriao)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.telAnfitriao)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.senhaAnfitriao)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.tokenAnfitiao)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.nomeAnfitriao)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.emailAnfitriao)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.celAnfitriao)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.telAnfitriao)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.senhaAnfitriao)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.tokenAnfitiao)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.AnfitriaoId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.AnfitriaoId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.AnfitriaoId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
